---
// Image used within .contentf contexts
// Most notably, max height is restricted to prevent images taking up too much vertical space and distrupting the flow of the page.

import Image from "@/components/img/Image.astro";
import { combineClasses } from "@/utils/markupUtils";

const props = Astro.props as any;
---

<Image class={combineClasses("contentf-lim-h", props.class)} {...props} />
